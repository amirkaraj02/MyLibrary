@model MyLibrary.Models.tblPersoneli
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/Layout.cshtml";
}

    <form class="form-group" method="post">
        <div style="margin-top:20px">
            <label>Emri Personelit</label>
            @* html helper ile yazar ismini alip db kaydedmek *@
            @Html.TextBoxFor(x => x.Emri, new { @class = "form-control" })
            @Html.ValidationMessageFor(x => x.Emri, "", new { @style = "color:forestgreen"})
        </div>
        <div style="margin-top:15px">
            <label>Mbiemri Personelit</label>
            @Html.TextBoxFor(x => x.Mbiemri, new { @class = "form-control" })
            @Html.ValidationMessageFor(x => x.Mbiemri, "", new { @style = "color:forestgreen"})
        </div>
        <div class="panel-body" style="margin-top:15px">
            <button type="submit" class="btn btn-outline-success">Rregjitro</button>
            <a href="/Personel/Index/" class="btn btn-outline-warning">Back</a>
        </div>
    </form>

